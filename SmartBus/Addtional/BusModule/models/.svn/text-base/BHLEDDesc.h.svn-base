#import <Foundation/Foundation.h>

@interface BHLEDDesc : NSObject

/* 线路ID */
@property (nonatomic, assign) NSInteger rid;

/* 线路名 */
@property (nonatomic, retain) NSString *rname;

/* 目的地 */
@property (nonatomic, retain) NSString *dest;

/* 编号 */
@property (nonatomic, assign) NSInteger busId;

/* 车即将到达的站台 */
@property (nonatomic, assign) NSInteger currentLevel;

/* 当前站台 */
@property (nonatomic, assign) NSInteger theLevel;

/* 距离 */
@property (nonatomic, assign) CGFloat distance;

/* 刷新时间 */
@property (nonatomic, assign) NSInteger rtime;

@end
